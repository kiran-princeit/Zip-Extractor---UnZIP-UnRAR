<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <include
        android:id="@+id/header"
        layout="@layout/header_layout" />

    <ImageView
        android:id="@+id/complete_logo"
        android:layout_width="800px"
        android:layout_height="721px"
        android:layout_centerInParent="true"
        android:src="@drawable/complete_logo" />

    <TextView
        android:id="@+id/msg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/complete_logo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_20sdp"
        android:gravity="center"
        android:textColor="#6a6a6a"
        android:textSize="@dimen/_15sdp" />

    <ImageView
        android:id="@+id/open_file"
        android:layout_width="620px"
        android:layout_height="130px"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginVertical="100px"
        android:layout_marginTop="100px"
        android:layout_marginBottom="100px"
        android:src="@drawable/effect_open_file" />
</RelativeLayout>
